    "function dec2bin(number) {\n",
    "    let bin = '';\n",
    "    while(number > 0) {\n",
    "          bin = number%2 + bin;\n",
    "          number = Math.floor(number/2);\n",
    "          }\n",
    "    return bin;\n",
    "}\n",
    "\n",
    "console.log(dec2bin(10))"
   ]
